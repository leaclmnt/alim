     newCanvas("choix_faim",500, 100)
        .add(0,20 , newText("Avez-vous faim en ce moment ? "))
        .add( 0, 50, newText("Oui") )
        .settings.add( 185, 50, newScale("reponse_oui", 1)
            .settings.callback( getScale("reponse_non").unselect() ))
        .add( 0, 70, newText("Non") )
        .settings.add( 185, 70, newScale("reponse_non", 1)
            .settings.callback( getScale("reponse_oui").unselect() ))
        .print()
