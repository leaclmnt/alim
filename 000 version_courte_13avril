Sequence( "intro", "intro2", "faim", randomize("experiment_1"), "encouragement", "experiment_2", "questions_generales", SendResults(), "bye")

// Intro

newTrial("intro",
    newCanvas("image", 450, 120)
        .center()
        .add(100, 10, newImage("APT.jpg"))
        .print()
    ,
    newCanvas("presentation_projet", 500, 270)
        .left()
        .add (0, 10, newText("Bonjour à tous,"))
        .add (0, 50, newText("Dans le cadre d'un projet de recherche &agrave; AgroParisTech, nous avons &eacute;labor&eacute; un questionnaire afin d'&eacute;tudier les habitudes alimentaires des consommateurs."))
        .add (0, 120, newText("Si vous pouvez prendre 15 minutes de votre temps pour r&eacute;pondre &agrave; nos questions, cela nous aiderait beaucoup dans nos recherches."))
        .add (0, 170, newText ("Le questionnaire est anonyme. Les donn&eacute;es collect&eacute;es seront stock&eacute;es dans une base de donn&eacute;es s&eacute;curis&eacute;e."))
        .add (0, 220, newText("Pour toute question ou commentaire, n'h&eacute;sitez pas &agrave; nous contacter &agrave; l'adresse suivante : solene.larcher@agroparistech.fr. "))
        .print()
    ,    
    newButton("Suivant")
        .print()
        .wait()
    )

