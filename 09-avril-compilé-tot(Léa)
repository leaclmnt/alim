        newSelector(row.GM)
            .settings.add( getScale("legume1_oui"), getScale("legume1_non") )
            .log()
        , 
        newSelector(row.GM)
            .settings.add( getScale("legume2_oui"), getScale("legume2_non") )
            .log()
