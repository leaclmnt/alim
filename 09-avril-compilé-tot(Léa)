            .print()
            .wait()
        , 
        newCanvas("entree", 450, 70)
            .center()
            .add(0, 10, newText("Choisissez &agrave; pr&eacute;sent une entr&eacute;e dans la liste ci-dessous :"))
            .add(20, 40, newDropDown("entree", "...")
                .add( row.entree1 )
                .add( row.entree2 )
                .add( row.entree3 )
                .add( row.entree4 )
                .add( row.pasentree ))
            .print()
