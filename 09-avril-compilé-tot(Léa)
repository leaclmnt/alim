            .settings.add( getScale("feculent_oui"), getScale("feculent_non") )
            .log(row.GM)
        , 
        newSelector(row.GM)
            .settings.add( getScale("feculentgras_oui"), getScale("feculentgras_non") )
            .log()
            .wait()
