            .add(20, 40, newDropDown("dessert1", "...")
                .add( row.fruit1 )
                .add( row.fruit2 )
                .add( row.laitage )
                .add( row.gourmand1 )
                .add( row.gourmand2 )
                .add( row.pasdessert ))
            .add(20, 65, newDropDown("dessert2", "...")
                .add( row.fruit1 )
                .add( row.fruit2 )
                .add( row.laitage )
                .add( row.gourmand1 )
                .add( row.gourmand2 )
                .add( row.pasdessert ))
            .print()
