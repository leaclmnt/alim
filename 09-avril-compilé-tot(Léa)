        .print()
    ,    
    newCanvas("presentation_projet2", 450, 220)
        .left()
        .add(0, 10, newText("Au cours de ce questionnaire, nous allons vous interroger sur l'&eacute;laboration d'une dizaine de menus, &agrave; chaque fois pour un repas &agrave; domicile."))
        .add(0, 70, newText("Pour chaque menu, vous allez &ecirc;tre amen&eacute; &agrave; choisir autant d'aliments que vous souhaitez pour composer votre plat principal."))
        .add(0, 110, newText("Ensuite, vous pourrez choisir d'accompagner votre plat d'une entr&eacute;e, d'une assiette de fromage et d'un ou deux desserts."))
        .add(0, 150, newText("Vous pourrez aussi choisir une boisson."))
        .add(0, 170, newText("Dans tous les cas, l’eau plate, le pain, les condiments et les &eacute;pices sont disponibles &agrave; volont&eacute;."))
        .print()
    ,
    newButton("Démarrer l'exp&eacute;rience")
        .print()
        .wait()
    )

