        , 
        getDropDown("dessert2").log(row.GM)
        ,
        newButton("Suivant")
            .print()
            .wait()
        , 
        newCanvas("boissons", 450, 90)
            .center()
            .add(0, 10, newText("L'eau est disponible &agrave; volont&eacute;. Souhaitez-vous accompagner votre plat d'une boisson ?"))
