newTrial("experiment_3", 
    newCanvas("regime", 450, 70)
        .center()
        .add(0, 10, newText("Quel est votre r&eacute;gime ?"))
        .add(20, 40, newDropDown("Regime", "...")
            .settings.add("Omnivore", "V&eacute;g&eacute;tarien"))
        .print()
    ,
    getDropDown("Regime").log("type_regime")
    ,
    newButton("Suivant")
        .print()
        .wait()
    ,
    getCanvas("regime").remove()
    ,
    getButton("Suivant").remove()
    ,
    getDropDown("Regime")
        .test.selected(1)
            
            .success(
                newCanvas("texte_vege", 450, 30)
                    .center()
                    .add(0, 10, newText("Pour chaque type de produits indiquez si vous en consommez ou non."))
