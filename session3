//Session 3

PenController (
    newText("Vous allez de nouveau composer 2 menus différents, à chaque fois pour un repas à domicile. Le fonctionnement est le même que précédemment.")
        .print()
)

Template( "TableSession3.csv" ,
    // Row will iteratively point to every row in bloc_1.csv 
    row => newTrial( "experiment_3" ,
        // The trials are minimal: choose a pronoun from a DropDown list
        newDropDown("choix_plat_1_session_3", "...")  
            .before( newText("Choissisez un plat parmi la liste suivante:") )    // Print the sentence to the left of the list
